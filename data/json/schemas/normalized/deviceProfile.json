{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "id",
    "networkTypeId",
    "companyId",
    "name",
    "networkSettings",
    "description"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "networkTypeId": {
      "$id": "#/properties/networkTypeId",
      "type": "integer",
      "title": "The Networktypeid Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "companyId": {
      "$id": "#/properties/companyId",
      "type": "integer",
      "title": "The Companyid Schema",
      "default": 0,
      "examples": [
        2
