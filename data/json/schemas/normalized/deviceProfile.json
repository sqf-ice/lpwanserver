          "type": "string",
          "title": "The Regparamsrevision Schema",
          "default": "",
          "examples": [
            "A"
          ],
          "pattern": "^(.*)$"
        },
        "rfRegion": {
          "$id": "#/properties/networkSettings/properties/rfRegion",
          "type": "string",
          "title": "The Rfregion Schema",
          "default": "",
          "examples": [
            "US902"
          ],
          "pattern": "^(.*)$"
        },
