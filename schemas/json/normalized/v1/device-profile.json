    "rxDelay1": {
      "type": "number",
      "description": ""
    },
    "rxFreq2": {
      "type": "number",
      "description": ""
    },
    "supports32BitFCnt": {
      "type": "boolean",
      "description": ""
    },
    "supportsClassB": {
      "type": "boolean",
      "description": ""
    },
    "supportsClassC": {
      "type": "boolean",
      "description": ""
